;; Database settings
{:database {:host "localhost"
            :port "5432"
            :name "db_name"
            :username "foobar"
            :password "testpwd1!"}
 ;; Temperature correction offset in degree Celsius
 :correction {:enabled true
              :offset 9}
 ;; Yubico OTP parameters
 :yubico-client-id "12345"
 :yubico-secret-key "xyz"
 ;; FMI API key
 :fmi-api-key "xyz"
 ;; Weather station ID
